<odoo>
    <data>
        <record id="account_payment_term_extension" model="ir.ui.view">
            <field name="name">Profisc:: account_payment_term_extension</field>
            <field name="model">account.payment.term</field>
            <field name="inherit_id" ref="account.view_payment_term_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='company_id']" position="after">
                    <field name="profisc_payment_code" string="Code"/>
                    <field name="profisc_payment_code_description" string="Code Description"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>