<odoo>
    <data>
        <record id="res_users_extension" model="ir.ui.view">
            <field name="name">Profisc:: res_users_extension</field>
            <field name="model">res.users</field>
            <field name="inherit_id" ref="base.view_users_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='active']" position="after">
                    <field name="profisc_operator_code" string="Operator Code"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>